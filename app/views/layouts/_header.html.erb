<header class="header-fixed header-login-signup">
  <div class="header-limiter">
    <h1>
      <a>
        <%= image_tag Settings.views.logo.url, size: Settings.views.logo.size_image %>
      </a>
    </h1>
    <nav>
      <a><%= link_to t("views.pages.home"), root_path %></a>
      <a><%= link_to t("views.pages.help"), help_path %></a>
    </nav>
    <nav class="pull-right">
      <a><%= link_to t("views.layouts.signin"), "#" %></a>
      <a><%= link_to t("views.layouts.signup"), "#" %></a>
    </nav>
  </div>
</header>
